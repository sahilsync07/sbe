    "@capacitor/filesystem": "^8.0.0",
    "@capacitor/share": "^8.0.0",
    "@cloudinary/url-gen": "^1.19.0",
    "@cloudinary/vue": "^1.0.5",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "axios": "^1.13.2",
    "file-saver": "^2.0.5",
    "jspdf": "^4.1.0",
    "jspdf-autotable": "^5.0.7",
    "jszip": "^3.10.1",
    "lucide-vue-next": "^0.562.0",
    "pdfjs-dist": "^5.4.530",
